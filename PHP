// Step 1: Tokenize sentence into words using regex
$words = preg_split("/[^a-zA-Z]+/", strtolower($sentence), -1, PREG_SPLIT_NO_EMPTY);

// Step 2: Count occurrences of each word
$wordCount = array_count_values($words);

