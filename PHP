print_r($_SESSION);

// (ii) Unset all session variables
session_unset();
echo "<h3>After session_unset():</h3>";
print_r($_SESSION);

// Re-register again for destroy demo
$_SESSION['username'] = "Jenitha";
$_SESSION['role'] = "Student";

// (iii) Destroy the session
session_destroy();

echo "<h3>Session destroyed.</h3>";
?>

